[Unit]
Description=Yubico Authenticator KRunner plugin
StartLimitIntervalSec=60s
StartLimitBurst=3

[Service]
PrivateTmp=yes
WorkingDirectory=/tmp
ExecStart=/usr/bin/krunner-yoath
Restart=on-failure
BusName=pl.kolosowscy.yoath
Slice=session.slice
TimeoutSec=40sec

[Install]
WantedBy=plasma-core.target
